#
#, c-format
msgid ""
"%d out of %d charts failed to download.\n"
"Check the list, verify there is a working Internet connection and repeat the "
"operation if needed."
msgstr ""
"%d dari %d grafik gagal diunduh.\n"
"Periksa daftar, verifikasi apakah ada koneksi Internet yang berfungsi dan "
"ulangi operasi jika diperlukan."
#
#, c-format
msgid "%lu charts total, %lu updated, %lu new"
msgstr "%lu total grafik, %lu diperbarui, %lu baru"
#
#, c-format
msgid "%s"
msgstr "%s"
#
#, c-format
msgid "%s (%s to %s), river miles %3.1f - %3.1f"
msgstr "%s (%s ke %s), mil sungai %3.1f - %3.1f"
#
#, c-format
msgid "%u charts total, %u updated, %u new"
msgstr "%u total grafik, %u diperbarui, %u baru"
#
msgid "(Please update first)"
msgstr "(Pertama silahkan update)"
#
msgid "0 (transfer completed)"
msgstr "0 (transfer selesai)"
#
msgid "0 (transfer paused)"
msgstr "0 (transfer dijeda)"
#
msgid "Abort"
msgstr "Menggugurkan"
#
msgid "Abort download"
msgstr "Menggugurkan download"
#
msgid "Add"
msgstr "Tambahkan"
#
msgid "Add a new chart catalog."
msgstr "Tambahkan katalog grafik baru."
#
msgid "After catalog update select for download"
msgstr "Setelah diperbarui katalog pilih untuk download"
#
msgid "All new charts"
msgstr "Semua grafik baru"
