msgstr "Grafik Downloader"
#
msgid "Chart Downloader PlugIn for OpenCPN"
msgstr "Plugin Grafik Downloader untuk OpenCPN"
#
msgid ""
"Chart Downloader PlugIn for OpenCPN\n"
"Manages chart downloads and updates from sources supporting\n"
"NOAA Chart Catalog format"
msgstr ""
"Grafik Downloader PlugIn untuk OpenCPN\n"
"Mengelola unduhan dan pembaruan grafik dari daftar berkas yang mendukung\n"
"NOAA Grafik Catalog"
#
msgid "Chart Downloader Preferences"
msgstr "Preferensi Grafik Downloader"
#
msgid "Chart source definition problem"
msgstr "Grafik sumber masalah definisi"
#
msgid "ChartDownloader"
msgstr "GrafikDownloader"
#
msgid "Charts"
msgstr "Grafik"
#
msgid "Choose Chart File Directory"
msgstr "Pilih Direktori File Grafik"
#
msgid "Close"
msgstr "Tutup"
#
msgid "Connection settings"
msgstr "Pengaturan koneksi"
