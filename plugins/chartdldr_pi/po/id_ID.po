msgid "Error"
msgstr "Kesalahan"
#
#, c-format
msgid "Error, the URL to the chart (%s) data seems wrong."
msgstr "Kesalahan, data URL ke grafik (%s) nampaknya salah."
#
msgid "Error, the URL to the chart source data seems wrong."
msgstr "Kesalahan, URL ke data sumber grafik nampaknya salah."
#
msgid "Estimated remaining time:"
msgstr "Perkiraan waktu yang tersisa:"
#
msgid "Estimated total time:"
msgstr "Perkiraan waktu total:"
#
#, c-format
msgid ""
"Failed to Download Catalog: %s \n"
"Verify there is a working Internet connection."
msgstr ""
"Gagal Download Katalog: %s \n"
"Verify ada koneksi Internet yang berfungsi."
#
#, c-format
msgid "Failed to Find New Catalog: %s "
msgstr "Gagal Menemukan Katalog Baru: %s "
